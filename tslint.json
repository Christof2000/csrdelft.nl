{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
		},
    "rules": {
			"indent": [true, "tabs"],
			"quotemark": [true, "single"],
			"object-literal-key-quotes": false,
			"space-before-function-paren": [true, {"anonymous": "always", "named": "never"}],
			"no-var-requires": false,
			"interface-name": false
		},
    "rulesDirectory": []
}

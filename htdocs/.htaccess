AddDefaultCharset utf-8
RewriteEngine On

RewriteRule ^plaetjes/(.*)$		tools/plaetjes.php?img=$1 [QSA,L]

RewriteRule ^(leden)?lijst/?(.*)$	tools/lijst.php [QSA,L]

RewriteRule ^profiel/([0-9a-z]+)/([a-zA-Z]+)/([a-zA-Z]+)/?$	tools/profiel.php?uid=$1&a=$2&s=$3 [QSA,L]
RewriteRule ^profiel/([0-9a-z]+)/([a-zA-Z]+)/?$				tools/profiel.php?uid=$1&a=$2 [QSA,L]
RewriteRule ^profiel/([0-9a-z]+)/?$							tools/profiel.php?uid=$1 [QSA,L]
RewriteRule ^profiel/?$										tools/profiel.php [QSA,L]

RewriteRule ^forum/rss/([0-9a-zA-Z]*).xml		index.php?c=Forum&private_token=$1 [L]
RewriteRule ^agenda/ical/([0-9a-zA-Z]*).ics		index.php?c=Agenda&private_token=$1 [L]
RewriteRule ^verify/([0-9a-zA-Z]*)				index.php?c=Login&onetime_token=$1 [L]

RewriteRule ^login				index.php?c=Login [L]
RewriteRule ^logout				index.php?c=Login [L]
RewriteRule ^su					index.php?c=Login [L]
RewriteRule ^endsu				index.php?c=Login [L]
RewriteRule ^pauper				index.php?c=Login [L]
RewriteRule ^wachtwoord			index.php?c=Login [L]
RewriteRule ^sessions			index.php?c=Login [L]
RewriteRule ^endsession			index.php?c=Login [L]

RewriteRule ^eetplan			index.php?c=Eetplan		[QSA,L]
RewriteRule ^documenten			index.php?c=Documenten	[QSA,L]
RewriteRule ^bibliotheek		index.php?c=Bibliotheek [QSA,L]
#RewriteRule ^groepen			index.php?c=GroepenRouter [L]
RewriteRule ^leden				index.php?c=Leden [QSA,L]
RewriteRule ^verticalen			index.php?c=Verticalen [L]
RewriteRule ^kringen			index.php?c=Verticalen [L]
RewriteRule ^maaltijden			index.php?c=MaalcieRouter [L]
RewriteRule ^corvee				index.php?c=MaalcieRouter [L]
RewriteRule ^forum				index.php?c=Forum [L]
RewriteRule ^agenda				index.php?c=Agenda [L]
RewriteRule ^courant			index.php?c=Courant [L]
RewriteRule ^fotoalbum			index.php?c=FotoAlbum [L]
